{
  "name": "@kong/konnect-portal",
  "version": "0.1.0",
  "private": false,
  "license": "Apache-2.0",
  "scripts": {
    "start": "yarn dev",
    "dev": "vite",
    "serve": "vite preview",
    "build": "vite build --mode production",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src",
    "commit": "cz",
    "build:watch": "vite build --watch",
    "build:watch:dev": "vite build --watch --mode development --minify false",
    "test": "yarn lint && yarn test:e2e",
    "test:e2e": "concurrently --kill-others \"yarn build:watch\" \"cypress open -C cypress.config.js --e2e -b chrome\" \"node cypress.server.mjs \"",
    "test:e2e:ci": "concurrently --success first --kill-others  \"yarn build && DEBUG=cypress:server cypress run -C cypress.config.js --e2e -b chrome\" \"node cypress.server.mjs \""
  },
  "dependencies": {
    "@kong-ui-public/copy-uuid": "^0.3.15",
    "@kong-ui-public/document-viewer": "^0.5.18",
    "@kong-ui-public/spec-renderer": "^0.7.4",
    "@kong/kong-auth-elements": "2.0.0",
    "@kong/kongponents": "8.60.1",
    "@kong/sdk-portal-js": "0.0.1-beta.20",
    "@xstate/vue": "^2.0.0",
    "axios": "0.27.2",
    "date-fns": "2.30.0",
    "js-yaml": "4.1.0",
    "markdown-it": "12.3.2",
    "markdown-it-anchor": "5.3.0",
    "markdown-it-table-of-contents": "0.6.0",
    "markdown-it-toc-done-right": "4.2.0",
    "pinia": "^2.0.33",
    "vite": "^4.2",
    "vue": "^3.2.47",
    "vue-router": "^4.2.1",
    "xstate": "4.37.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.59.7",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-standard": "5.1.2",
    "@vue/eslint-config-typescript": "11.0.3",
    "commitizen": "^4.3.0",
    "concurrently": "7.6.0",
    "cypress": "12.11.0",
    "cypress-split": "^1.3.8",
    "cypress-terminal-report": "4.1.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.35.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-portal-vue": "file:./eslint-plugin-portal-vue",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-vue": "^9.14.1",
    "express": "^4.18.2",
    "lefthook": "^1.4.1",
    "sass": "1.58.3",
    "tailwindcss": "1.9.6",
    "typescript": "4.9.5",
    "vite-svg-loader": "^3.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "npm": "please-use-yarn",
    "node": ">= 16.19.0",
    "yarn": ">= 1.22.17"
  },
  "volta": {
    "node": "16.19.0",
    "yarn": "1.22.17"
  }
}
